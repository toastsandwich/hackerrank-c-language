//this code will help you to sum all digits of a number upto 100 digits just change the i < 6 to i < 101

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n,i,var,sum=0;
    scanf("%d", &n);
    
    while (i < 6){
        
        var = n % 10;
        
        sum = sum + var;
        
        n = n / 10;
        
        i++;
    }
    
    printf("%d",sum);
    
    
    return 0;
}
